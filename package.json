{
  "name": "web-site",
  "version": "0.0.1",
  "scripts": {
    "start": "node app",
    "dev": "npm-run-all --parallel watch:*",
    "watch:webpack": "cross-env NODE_ENV=development webpack --colors --watch --display-error-details",
    "watch:server": "nodemon --exec \"node app.js\"",
    "watch:style": "less-watch-compiler less public/stylesheets --config=less-watch-compiler.development.json",
    "build": "npm run build:style && npm run build:js",
    "build:style": "less-watch-compiler less public/stylesheets --config=less-watch-compiler.production.json",
    "build:js": "cross-env NODE_ENV=production webpack --colors --display-error-details"
  },
  "description": "web-site template",
  "author": {
    "name": "IT PEPPER"
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "crypto-js": "^3.1.9-1",
    "ejs": "^2.6.1",
    "express": "^4.14.0",
    "flow-bin": "^0.85.0",
    "history": "^4.7.2",
    "js-cookie": "^2.2.0",
    "jsonfile": "^5.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "restler": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-babel-webpack-plugin": "^1.1.1",
    "less": "^3.8.1",
    "less-watch-compiler": "^1.13.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2"
  }
}
